@model IEnumerable<UCP1_105.Models.Universitas>

@{
    ViewData["Title"] = "Index";
}

<h1>Universitas</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NamaUniv)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdFakultasNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdProdiNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NamaUniv)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdFakultasNavigation.IdFakultas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdProdiNavigation.IdProdi)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdUniv"><i class="fa fa-marker fa-lg"></i></a> |
                <a asp-action="Details" asp-route-id="@item.IdUniv"><i class="fas fa-list"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.IdUniv"><i class="fa fa-trash-alt fa-lg"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
